  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Dynamics.Commerce.HardwareStation.Localization" Version="$(HardwareRepoPackagesVersion)" />
  </ItemGroup>
  <Import Project="..\..\..\BuildTools\Microsoft.Dynamics.RetailSdk.Build.targets" />
