    <ProjectReference Include="..\..\..\OnlineStore\Ecommerce.Sdk.Core\Platform.Ecommerce.Sdk.Core.csproj" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="System.Composition" Version="1.2.0" />
