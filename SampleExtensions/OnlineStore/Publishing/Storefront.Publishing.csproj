    <TargetFramework>net461</TargetFramework>
    <AssemblyName>$(AssemblyNamePrefix).Retail.Ecommerce.Publishing</AssemblyName>
    <RootNamespace>Contoso.Retail.Ecommerce.Publishing</RootNamespace>
